<Window x:Class="CESDKLicenseCreator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Steam Liscense Creator" Height="200" Width="200" Loaded="Window_Loaded" Icon="/CESDKLicenseCreator;component/Icon.ico">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="261*" />
        </Grid.ColumnDefinitions>
        <Label Content="Steam ID#" Height="28" HorizontalAlignment="Left" Margin="54,12,0,0" Name="label1" VerticalAlignment="Top" Grid.Column="1" Width="67" />
        <TextBox Grid.Column="1" Height="23" HorizontalAlignment="Left" Margin="44,46,0,0" Name="textBox1" VerticalAlignment="Top" Width="91" />
        <Button Click="button1_Click" Content="License" Height="23" HorizontalAlignment="Left" Margin="55,111,0,0" Name="button1" VerticalAlignment="Top" Width="66" Grid.Column="1" />
    </Grid>
</Window>
